{
  "name": "johansports-assignment",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "adb": "adb reverse tcp:8081 tcp:8081",
    "android": "expo start --android",
    "android:bundle:debug": "react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:bundle:release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/",
    "ci": "yarn prettier-ci && yarn lint-ci && yarn ts-check",
    "create-android-package": "cd android && ./gradlew assembleRelease",
    "create-ios-package": "cd ios && xcodebuild -sdk iphonesimulator -workspace Mpower.xcworkspace/ -scheme Mpower -configuration Release",
    "ios": "expo start --ios",
    "ios:bundle:release": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "lint": "esw . --ext .js,.jsx,.ts,.tsx --fix --color",
    "lint-ci": "esw . --ext .js,.jsx,.ts,.tsx --color",
    "precommit": "yarn prettier && yarn lint && yarn ts-check",
    "prettier": "prettier --write .",
    "prettier-ci": "prettier -c .",
    "start": "expo start --dev-client",
    "start_tunnel": "expo start --tunnel",
    "test": "jest --passWithNoTests",
    "ts-check": "tsc --noEmit",
    "web": "expo start --web"
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/stack": "6.3.16",
    "axios": "1.3.6",
    "expo": "~48.0.10",
    "expo-image": "~1.0.0",
    "expo-status-bar": "~1.4.4",
    "i18n-js": "4.2.3",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-gesture-handler": "2.9.0",
    "react-native-paper": "5.7.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/eslint-parser": "7.21.3",
    "@babel/runtime": "7.21.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@types/react-native": "~0.71.6",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "babel-plugin-inline-import": "3.0.0",
    "eslint": "8.38.0",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "eslint-watch": "8.0.0",
    "pre-commit": "1.2.2",
    "prettier": "2.8.7",
    "typescript": "4.9.5"
  }
}
